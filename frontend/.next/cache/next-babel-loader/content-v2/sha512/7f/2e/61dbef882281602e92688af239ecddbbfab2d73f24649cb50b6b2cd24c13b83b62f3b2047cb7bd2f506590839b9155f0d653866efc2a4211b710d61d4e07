{"ast":null,"code":"import fetch from 'isomorphic-fetch';\nimport { API } from '../config';\nexport var createReservation = function createReservation(reservation) {\n  console.log('dans action', reservation);\n  return fetch(\"\".concat(API, \"/reservation\"), {\n    method: 'POST',\n    headers: {\n      'Content-Type': 'application/json'\n    },\n    body: JSON.stringify(reservation)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var createContract = function createContract(reservation, token) {\n  console.log('dans action createContract', reservation);\n  return fetch(\"\".concat(API, \"/reservation/contract/:reservation\"), {\n    method: 'POST',\n    headers: {\n      'Content-Type': 'application/json',\n      Authorization: \"Bearer \".concat(token)\n    },\n    body: JSON.stringify(reservation)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var listAllReservations = function listAllReservations(token) {\n  return fetch(\"\".concat(API, \"/reservation\"), {\n    method: 'GET',\n    headers: {\n      Authorization: \"Bearer \".concat(token)\n    }\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var countNouvelleReservation = function countNouvelleReservation() {\n  return fetch(\"\".concat(API, \"/reservation/count\"), {\n    method: 'GET'\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var listeOneReservation = function listeOneReservation(id, token) {\n  return fetch(\"\".concat(API, \"/reservation/\").concat(id), {\n    method: 'GET',\n    headers: {\n      Authorization: \"Bearer \".concat(token)\n    }\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var removeReservation = function removeReservation(id, token) {\n  return fetch(\"\".concat(API, \"/reservation/\").concat(id), {\n    method: 'DELETE',\n    headers: {\n      Authorization: \"Bearer \".concat(token)\n    }\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var updateReservation = function updateReservation(reservation, id, token) {\n  return fetch(\"\".concat(API, \"/reservation/\").concat(id), {\n    method: 'PUT',\n    headers: {\n      'Content-Type': 'application/json',\n      Authorization: \"Bearer \".concat(token)\n    },\n    body: JSON.stringify(reservation)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};","map":{"version":3,"sources":["C:/Users/jossi/Dropbox/JOSSIC/6-development/Devs/gites-wao/frontend/actions/reservationActions.js"],"names":["fetch","API","createReservation","reservation","console","log","method","headers","body","JSON","stringify","then","response","json","err","createContract","token","Authorization","listAllReservations","countNouvelleReservation","listeOneReservation","id","removeReservation","updateReservation"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,kBAAlB;AACA,SAASC,GAAT,QAAoB,WAApB;AAEA,OAAO,IAAMC,iBAAiB,GAAG,SAApBA,iBAAoB,CAACC,WAAD,EAAiB;AACjDC,EAAAA,OAAO,CAACC,GAAR,CAAY,aAAZ,EAA2BF,WAA3B;AACA,SAAOH,KAAK,WAAIC,GAAJ,mBAAuB;AAClCK,IAAAA,MAAM,EAAE,MAD0B;AAElCC,IAAAA,OAAO,EAAE;AACR,sBAAgB;AADR,KAFyB;AAKlCC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,WAAf;AAL4B,GAAvB,CAAL,CAOLQ,IAPK,CAOA,UAACC,QAAD,EAAc;AACnB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACA,GATK,WAUC,UAACC,GAAD;AAAA,WAASV,OAAO,CAACC,GAAR,CAAYS,GAAZ,CAAT;AAAA,GAVD,CAAP;AAWA,CAbM;AAcP,OAAO,IAAMC,cAAc,GAAG,SAAjBA,cAAiB,CAACZ,WAAD,EAAca,KAAd,EAAwB;AACrDZ,EAAAA,OAAO,CAACC,GAAR,CAAY,4BAAZ,EAA0CF,WAA1C;AACA,SAAOH,KAAK,WAAIC,GAAJ,y
\r\n\t\t\t\t\t\tif (nuitee === 1) {\r\n\t\t\t\t\t\t\tconsole.log(\r\n\t\t\t\t\t\t\t\t'Cas week-end 1 nuit + msg avertissement'\r\n\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t//\r\n\t\t\t\t\t\t\tsetValues({\r\n\t\t\t\t\t\t\t\t...values,\r\n\t\t\t\t\t\t\t\tprixTotal:\r\n\t\t\t\t\t\t\t\t\tgite.tarifParPersParNuitftMenage +\r\n\t\t\t\t\t\t\t\t\tgite.tarifDeBase / 2 +\r\n\t\t\t\t\t\t\t\t\tgite.uneNuitee,\r\n\t\t\t\t\t\t\t\tavertissement:\r\n\t\t\t\t\t\t\t\t\t'En week end nous louons 2 nuitées, (très exceptionnelement 1 seule)',\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t} else if (nuitee === 2) {\r\n\t\t\t\t\t\t\tconsole.log('Cas week-end 2 nuits');\r\n\t\t\t\t\t\t\t//\r\n\t\t\t\t\t\t\tsetValues({\r\n\t\t\t\t\t\t\t\t...values,\r\n\t\t\t\t\t\t\t\tprixTotal:\r\n\t\t\t\t\t\t\t\t\tgite.tarifParPersParNuitftMenage +\r\n\t\t\t\t\t\t\t\t\tgite.tarifDeBase,\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t} else if (nuitee === 3) {\r\n\t\t\t\t\t\t\tconsole.log('Cas week-end 3 nuits');\r\n\t\t\t\t\t\t\t//\r\n\t\t\t\t\t\t\tsetValues({\r\n\t\t\t\t\t\t\t\t...values,\r\n\t\t\t\t\t\t\t\tprixTotal:\r\n\t\t\t\t\t\t\t\t\tgite.tarifParPersParNuitftMenage +\r\n\t\t\t\t\t\t\t\t\tgite.tarifDeBase +\r\n\t\t\t\t\t\t\t\t\tgite.troisNuitees,\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t} else if (nuitee === 4) {\r\n\t\t\t\t\t\t\tconsole.log('Cas week-end 4 nuits');\r\n\t\t\t\t\t\t\t//\r\n\t\t\t\t\t\t\tsetValues({\r\n\t\t\t\t\t\t\t\t...values,\r\n\t\t\t\t\t\t\t\tprixTotal:\r\n\t\t\t\t\t\t\t\t\tgite.tarifParPersParNuitftMenage +\r\n\t\t\t\t\t\t\t\t\tgite.tarifDeBase +\r\n\t\t\t\t\t\t\t\t\tgite.quatreNuitees,\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t} else if (nuitee === 5) {\r\n\t\t\t\t\t\t\tconsole.log(\r\n\t\t\t\t\t\t\t\t'Cas week-end 5 nuits - Quel tarif ? Voir Maman'\r\n\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t//\r\n\t\t\t\t\t\t\tsetValues({ ...values, prixTotal });\r\n\t\t\t\t\t\t} else if (nuitee === 6) {\r\n\t\t\t\t\t\t\tconsole.log(\r\n\t\t\t\t\t\t\t\t'Cas week-end 6 nuits - Quel tarif ? Voir Maman'\r\n\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t//\
n  if (false) {\n    const cookieChecked = getCookie('token');\n\n    if (cookieChecked) {\n      if (localStorage.getItem('user')) {\n        return JSON.parse(localStorage.getItem('user'));\n      } else {\n        return false;\n      }\n    }\n  }\n};","map":{"version":3,"sources":["C:/Users/gites/Dropbox/gites-wao/frontend/actions/authActions.js"],"names":["fetch","API","cookie","Router","handleResponse","response","status","signout","push","pathname","query","message","login","user","method","headers","Accept","body","JSON","stringify","then","json","catch","err","console","log","logout","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","name","email","isAdmin","_id","isAuth","cookieChecked","getItem","parse"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,kBAAlB;AACA,SAASC,GAAT,QAAoB,WAApB;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,OAAOC,MAAP,MAAmB,aAAnB;AAEA,OAAO,MAAMC,cAAc,GAAIC,QAAD,IAAc;AAC3C,MAAIA,QAAQ,CAACC,MAAT,KAAoB,GAAxB,EAA6B;AAC5BC,IAAAA,OAAO,CAAC,MAAM;AACbJ,MAAAA,MAAM,CAACK,IAAP,CAAY;AACXC,QAAAA,QAAQ,EAAE,QADC;AAEXC,QAAAA,KAAK,EAAE;AACNC,UAAAA,OAAO,EACN;AAFK;AAFI,OAAZ;AAOA,KARM,CAAP;AASA,GAVD,MAUO;AACN;AACA;AACD,CAdM;AAgBP,OAAO,MAAMC,KAAK,GAAIC,IAAD,IAAU;AAC9B,SAAOb,KAAK,CAAE,GAAEC,GAAI,cAAR,EAAuB;AAClCa,IAAAA,MAAM,EAAE,MAD0B;AAElCC,IAAAA,OAAO,EAAE;AACRC,MAAAA,MAAM,EAAE,kBADA;AAER,sBAAgB;AAFR,KAFyB;AAMlCC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeN,IAAf;AAN4B,GAAvB,CAAL,CAQLO,IARK,CAQCf,QAAD,IAAc;AACnB,WAAOA,QAAQ,CAACgB,IAAT,EAAP;AACA,GAVK,EAWLC,KAXK,CAWEC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CAXV,CAAP;AAYA,CAbM;AAeP,OAAO,MAAMG,MAAM,GAAIC,IAAD,IAAU;AAC/BC,EAAAA,YAAY,CAAC,OAAD,CAAZ;AACAC,EAAAA,kBAAkB,CAAC,MAAD,CAAlB;AACAF,EAAAA,IAAI;AAEJ,SAAO3B,KAAK,CAAE,GAAEC,GAAI,eAAR,EAAwB;AACnCa,IAAAA,MAAM,EAAE;AAD2B,GAAxB,CAAL,CAGLM,IAHK,CAGCf,QAAD,IAAc;AACnBmB,IAAAA,OAAO,CAACC,GAAR,CAAY,iCAAZ;AACA,GALK,EAMLH,KANK,CAMEC,GAAD,IAAS;AACfC,IAAAA
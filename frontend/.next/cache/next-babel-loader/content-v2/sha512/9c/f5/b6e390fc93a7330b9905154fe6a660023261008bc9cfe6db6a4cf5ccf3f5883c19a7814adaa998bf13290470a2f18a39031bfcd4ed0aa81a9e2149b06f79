{"ast":null,"code":"import { QR_LIST_REQUEST, QR_LIST_SUCCESS, QR_LIST_FAIL // QR_DETAILS_REQUEST,\n// QR_DETAILS_SUCCESS,\n// QR_DETAILS_FAIL,\n, QR_DELETE_REQUEST, QR_DELETE_SUCCESS, QR_DELETE_FAIL, QR_CREATE_REQUEST, QR_CREATE_SUCCESS, QR_CREATE_FAIL, QR_CREATE_RESET, QR_UPDATE_REQUEST, QR_UPDATE_SUCCESS, QR_UPDATE_FAIL, QR_UPDATE_RESET } from '../constants/giteConstants';\nexport const QRListReducer = (state = {\n  QRs: []\n}, action) => {\n  switch (action.type) {\n    case QR_LIST_REQUEST:\n      return {\n        loading: true,\n        QRs: []\n      };\n\n    case QR_LIST_SUCCESS:\n      return {\n        loading: false,\n        QRs: action.payload.QRs // pages: action.payload.pages,\n        // page: action.payload.page,\n\n      };\n\n    case QR_LIST_FAIL:\n      return {\n        loading: false,\n        error: action.payload\n      };\n\n    default:\n      return state;\n  }\n};\nexport const QRDeleteReducer = (state = {}, action) => {\n  switch (action.type) {\n    case QR_DELETE_REQUEST:\n      return {\n        loading: true\n      };\n\n    case QR_DELETE_SUCCESS:\n      return {\n        loading: false,\n        success: true\n      };\n\n    case QR_DELETE_FAIL:\n      return {\n        loading: false,\n        error: action.payload\n      };\n\n    default:\n      return state;\n  }\n};\nexport const QRCreateReducer = (state = {}, action) => {\n  switch (action.type) {\n    case QR_CREATE_REQUEST:\n      return {\n        loading: true\n      };\n\n    case QR_CREATE_SUCCESS:\n      return {\n        loading: false,\n        success: true,\n        QR: action.payload\n      };\n\n    case QR_CREATE_FAIL:\n      return {\n        loading: false,\n        error: action.payload\n      };\n\n    case QR_CREATE_RESET:\n      return {};\n\n    default:\n      return state;\n  }\n};\nexport const QRUpdateReducer = (state = {\n  QR: {}\n}, action) => {\n  switch (action.type) {\n    case QR_UPDATE_REQUEST:\n      return {\n        loading: true\n      };\n\n    case QR_UPDATE_SUCCESS:\n      return {\n        loading: false,\n        success: true,\n        QR: action.payload\n      };\n\n    case QR_UPDATE_FAIL:\n      return {\n        loading: false,\n        error: action.payload\n      };\n\n    case QR_UPDATE_RESET:\n      return {\n        QR: {}\n      };\n\n    default:\n      return state;\n  }\n};","map":{"version":3,"sources":["C:/Users/jossi/Dropbox/JOSSIC/6-development/Devs/gites-wao/frontend/reducers/giteReducers.js"],"names":["QR_LIST_REQUEST","QR_LIST_SUCCESS","QR_LIST_FAIL","QR_DELETE_REQUEST","QR_DELETE_SUCCESS","QR_DELETE_FAIL","QR_CREATE_REQUEST","QR_CREATE_SUCCESS","QR_CREATE_FAIL","QR_CREATE_RESET","QR_UPDATE_REQUEST","QR_UPDATE_SUCCESS","QR_UPDATE_FAIL","QR_UPDATE_RESET","QRListReducer","state","QRs","action","type","loading","payload","error","QRDeleteReducer","success","QRCreateReducer","QR","QRUpdateReducer"],"mappings":"AAAA,SACCA,eADD,EAECC,eAFD,EAGCC,YAHD,CAIC;AACA;AACA;AAND,EAOCC,iBAPD,EAQCC,iBARD,EASCC,cATD,EAUCC,iBAVD,EAWCC,iBAXD,EAYCC,cAZD,EAaCC,eAbD,EAcCC,iBAdD,EAeCC,iBAfD,EAgBCC,cAhBD,EAiBCC,eAjBD,QAkBO,4BAlBP;AAoBA,OAAO,MAAMC,aAAa,GAAG,CAACC,KAAK,GAAG;AAAEC,EAAAA,GAAG,EAAE;AAAP,CAAT,EAAsBC,MAAtB,KAAiC;AAC7D,UAAQA,MAAM,CAACC,IAAf;AACC,SAAKlB,eAAL;AACC,aAAO;AAAEmB,QAAAA,OAAO,EAAE,IAAX;AAAiBH,QAAAA,GAAG,EAAE;AAAtB,OAAP;;AACD,SAAKf,eAAL;AACC,aAAO;AACNkB,QAAAA,OAAO,EAAE,KADH;AAENH,QAAAA,GAAG,EAAEC,MAAM,CAACG,OAAP,CAAeJ,GAFd,CAGN;AACA;;AAJM,OAAP;;AAMD,SAAKd,YAAL;AACC,aAAO;AAAEiB,QAAAA,OAAO,EAAE,KAAX;AAAkBE,QAAAA,KAAK,EAAEJ,MAAM,CAACG;AAAhC,OAAP;;AACD;AACC,aAAOL,KAAP;AAbF;AAeA,CAhBM;AAkBP,OAAO,MAAMO,eAAe,GAAG,CAACP,KAAK,GAAG,EAAT,EAAaE,MAAb,KAAwB;AACtD,UAAQA,MAAM,CAACC,IAAf;AACC,SAAKf,iBAAL;AACC,aAAO;AACNgB,QAAAA,OAAO,EAAE;AADH,OAAP;;AAGD,SAAKf,iBAAL;AACC,aAAO;AACNe,QAAAA,OAAO,EAAE,KADH;AAENI,QAAAA,OAAO,EAAE;AAFH,OAAP;;AAID,SAAKlB,cAAL;AACC,aAAO;AACNc,QAAAA,OAAO,EAAE,KADH;AAENE,QAAAA,KAAK,EAAEJ,MAAM,CAACG;AAFR,OAAP;;AAID;AACC,aAAOL,KAAP;AAhBF;AAkBA,CAnBM;AAqBP,OAAO,MAAMS,eAAe,GAAG,C